{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    // Custom code style
    "airbnb-base",
    // Settings for Prettier
    "prettier/@typescript-eslint",
    "plugin:prettier/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "import/extensions": 0,
    "import/no-unresolved": 0,
    "no-console": 0,
    "no-param-reassign": 0,
    "import/no-extraneous-dependencies" : 0
  }
}
